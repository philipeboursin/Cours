   "source": [
    "**1.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[x^2 - 2*x*z + 5, x*z^3 + 9/640*z^8 - 3/20*z^7 + 3/16*z^5, y^2 - 8/3*z^3, y*z^3 - 3/80*z^8 + 2/5*z^7 - 1/2*z^5, z^9 - 32/3*z^8 + 80/3*z^6 + 1600/9*z^3]\n"
     ]
    }
   ],
   "source": [
    "A.<x, y, z> = PolynomialRing(QQ, order = 'lex')\n",
    "I = A.ideal(x^2 - 2*x*z + 5, x*y^2 + y*z^3, 3*y^2 - 8*z^3)\n",
    "G = I.groebner_basis()\n",
    "print(G)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "**2.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[y^4 + 16/3*y^3*z + 320/9*y^2, x*y^2 + 3/8*y^3, z^3 - 3/8*y^2, x^2 - 2*x*z + 5]\n"
     ]
    }
   ],
   "source": [
    "A = A.change_ring(order = 'degrevlex')\n",
    "I = I.change_ring(A)\n",
    "G = I.groebner_basis()\n",
    "print(G)"
   ]
  }
